<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.image.ImageView?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.image.Image?>
<HBox fx:id="savedFormsParent" prefHeight="26.0" prefWidth="342.0" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
        <children>
            <Label fx:id="savedForms" prefHeight="25.0" prefWidth="306.0" text="Form1, form2, form3, form4">
                <padding>
                    <Insets top="5.0" />
                </padding>
                <opaqueInsets>
                    <Insets />
                </opaqueInsets>
            </Label>
            <Button fx:id="editButton" mnemonicParsing="false" onAction="#editWordForms" prefHeight="33.0" prefWidth="33.0">
                <HBox.margin>
                    <Insets left="5.0" right="3.0" />
                </HBox.margin>
                <graphic>
                    <ImageView fitHeight="21.0" fitWidth="21.0" pickOnBounds="true" preserveRatio="true">
                        <image>
                            <Image url="@assets/edit.png" />
                        </image>
                    </ImageView>
                </graphic>
            </Button>
            <Button fx:id="deleteButton" mnemonicParsing="false" onAction="#deleteWordForms" prefHeight="25.0" prefWidth="17.0">
                <HBox.margin>
                    <Insets left="3.0" right="3.0" />
                </HBox.margin>
                <graphic>
                    <ImageView fitHeight="25.0" fitWidth="17.0" pickOnBounds="true" preserveRatio="true">
                        <image>
                            <Image url="@assets/trash0.png" />
                        </image>
                    </ImageView>
                </graphic>
            </Button>
        </children>
    </HBox>